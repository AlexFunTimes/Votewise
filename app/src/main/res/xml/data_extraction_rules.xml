<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive user data from cloud backup -->
        <exclude domain="sharedpref" path="user_preferences.xml"/>
        <exclude domain="database" path="votewise_database"/>
    </cloud-backup>
    <device-transfer>
        <!-- Allow device transfer for non-sensitive data -->
        <include domain="sharedpref" path="app_settings.xml"/>
    </device-transfer>
</data-extraction-rules>